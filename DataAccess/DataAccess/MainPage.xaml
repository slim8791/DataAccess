<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:DataAccess"
             x:Class="DataAccess.MainPage">
    <ContentPage.Content>
        <TableView>
            <TableRoot>
                <TableSection>
                    <EntryCell x:Name="MyEntry" Label="To Do" Placeholder="Put your settings here" Completed="OnChage"></EntryCell>
                    <SwitchCell x:Name="MySwitch" OnChanged="OnChage"></SwitchCell>
                </TableSection>
            </TableRoot>
        </TableView>
    </ContentPage.Content>
</ContentPage>